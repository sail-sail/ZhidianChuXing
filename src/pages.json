{
	"easycom": {
		"autoscan": true,
		"custom": {
      "^App(.*)": "@/components/App$1/App$1.vue",
      "^Custom(.*)": "@/components/Custom$1/Custom$1.vue",
      "^tm-(.*)": "@/tmui/components/tm-$1/tm-$1.vue"
		}
	},
	"pages": [
		{
			"path": "pages/index/index",
			"style": {
        "navigationStyle": "custom",
				"navigationBarTitleText": "质电出行",
				"transparentTitle": "always"
			}
		},
		{
			"path": "pages/baseInfo/index",
			"style": {
				"navigationBarTitleText": "基本信息",
				"transparentTitle": "always"
			}
		},
		{
			"path": "pages/my/index",
			"style": {
				"navigationBarTitleText": "个人中心",
				"transparentTitle": "always"
			}
		},
		{
			"path": "pages/idCard/index",
			"style": {
				"navigationBarTitleText": "身份证认证",
				"transparentTitle": "always"
			}
		},
		{
			"path": "pages/driverCard/index",
			"style": {
				"navigationBarTitleText": "驾驶证认证",
				"transparentTitle": "always"
			}
		},
		{
			"path": "pages/orderList/index",
			"style": {
				"navigationBarTitleText": "订单列表",
				"transparentTitle": "always"
			}
		}
	],
  "tabBar": {
    "color": "#757575",
    "selectedColor": "#0163FF",
    "list": [
      {
        "pagePath": "pages/index/index",
        "text": "首页",
        "iconPath": "static/tabbar/home.png",
        "selectedIconPath": "static/tabbar/home-active.png"
      },
      {
        "pagePath": "pages/my/index",
        "text": "个人中心",
        "iconPath": "static/tabbar/my.png",
        "selectedIconPath": "static/tabbar/my-active.png"
      }
    ]
  },
	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "uni-app",
		"navigationBarBackgroundColor": "#FFFFFF",
		"backgroundColor": "#FFFFFF"
	},
	"condition": {
		"current": 0,
		"list": [
			{
				"name": "",
				"path": "",
				"query": ""
			}
		]
	}
}
